# Firebase Functions Environment Variables
# Copy to .env and fill in your values
# Or use: firebase functions:config:set key=value

# Twilio
TWILIO_ACCOUNT_SID=AC...
TWILIO_AUTH_TOKEN=...

# Anthropic (Claude)
ANTHROPIC_API_KEY=sk-ant-...

# Google OAuth
GOOGLE_CLIENT_ID=...apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=...
GOOGLE_REDIRECT_URI=https://your-project.web.app/auth/google/callback

# Stripe
STRIPE_SECRET_KEY=sk_live_...
STRIPE_WEBHOOK_SECRET=whsec_...

# Deploy with Firebase config:
# firebase functions:config:set twilio.account_sid="AC..." twilio.auth_token="..."
# firebase functions:config:set anthropic.api_key="sk-ant-..."
# firebase functions:config:set google.client_id="..." google.client_secret="..." google.redirect_uri="..."
# firebase functions:config:set stripe.secret_key="sk_..." stripe.webhook_secret="whsec_..."
